# launch authentication agent
/usr/lib/polkit-1/polkit-agent-helper-1 &

# Top Bar
sh ~/.config/polybar/launch-openbox.sh &

## Desktop notification
/usr/lib/xfce4/notifyd/xfce4-notifyd &

# start compositor
picom &

# start urxvt terminal daemon
urxvtd -q &

# Set keyboard layout
setxkbmap de

# Keyboard Shortuts Manager
sxhkd &

# Set background image
nitrogen --restore &

# Display Systeminfos on desktop
conky &

# Applets
nm-applet &
volumeicon &
xfce4-clipman &
xfce4-power-manager &
blueman-applet &

# start dock 
plank &

# autostarting apps
obsidian &
syncthing-gtk &


